    "supervillains": "^1.0.0",
    "unist-util-visit": "^1.1.3"
  },
  "scripts": {
    "dev": "NODE_ENV=development nodemon -w graphql -w src -w lib -w .env -w server.js -w index.js index.js --preserve-symlinks",
    "start": "node index.js",
    "precommit": "lint-staged",
    "standardize": "standard --fix",
    "migrate:up": "db-migrate up",
    "migrate:down": "db-migrate down",
    "db:seed": "cat seeds/seeds.json | node seeds/seed.js --truncate",
    "db:reset:noseeds": "db-migrate reset && db-migrate up",
    "db:reset": "npm run db:reset:noseeds && npm run db:seed",
    "translations": "gsheets --key=1_1g3sW8u1aD9xGRfS479s0Pjwx0LxYRUEhETGkKlwLE --title=live --pretty --out lib/translations.json",
    "test:travis": "istanbul cover tape \"?(__tests__|lib|graphql)/**/*.test.js\" --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "test": "export DATABASE_URL=postgres://localhost:5432/publikator-test && echo $DATABASE_URL && dropdb --if-exists publikator-test && createdb publikator-test && db-migrate reset && db-migrate up && NODE_ENV=testing istanbul cover tape \"?(__tests__|lib|graphql)/**/*.test.js\"",
    "pull:redis": "node script/pullRedis.js"
  },
  "lint-staged": {
    "*.js": [
      "standard --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "gsheets": "^2.0.0",
    "husky": "^0.14.3",
    "istanbul": "^0.4.5",
    "lint-staged": "^4.3.0",
    "nodemon": "^1.12.1",
    "standard": "^10.0.3",
    "tape-async": "^2.3.0",
    "ws": "^3.3.0"
  }
}
