    "cron": "^1.3.0",
    "d3-array": "^1.2.1",
    "d3-time-format": "^2.1.0",
    "data-uri-to-buffer": "^2.0.0",
    "db-migrate": "^0.10.0-beta.24",
    "db-migrate-pg": "^0.2.5",
    "deep-diff": "^0.3.8",
    "dotenv": "^4.0.0",
    "export-files": "^2.1.1",
    "file-type": "^7.2.0",
    "github": "^12.0.1",
    "graphql": "^0.11.7",
    "graphql-tools": "github:orbiting/graphql-tools",
    "graphql-type-json": "^0.1.4",
    "immutable": "^3.8.2",
